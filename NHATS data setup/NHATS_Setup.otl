	replace sr_hip_raw=0 if hc`w'brokebon1==2 & wave==`w'
	replace sr_hip_raw = -7 if hc`w'brokebon1==-7 & wave==`w'
	replace sr_hip_raw = -8 if hc`w'brokebon1==-8 & wave==`w'
