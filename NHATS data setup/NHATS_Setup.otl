//merge with nhats survey data

use spid using "E:\nhats\data\nhats cleaned\sp_round_1_6.dta", clear
duplicates drop
merge 1:m spid using "E:\nhats\data\CMS_DUA_28016\mbsf_dod.dta", nogen ///
keepusing(spid death_date admit)
duplicates drop 
rename death_date dd_claims

by spid, sort: drop if missing(dd) & _N>1
forvalues i=2/6 {
	merge 1:1 spid using "E:\nhats\data\NHATS Public\round_`i'\nhats_round_`i'_tracker_file", ///
	nogen keep(match master) keepusing(r`i'status r`i'spstatdt*)
	merge 1:1 spid using "E:\nhats\data\NHATS Sensitive\r`i'_sensitive\nhats_round_`i'_sp_sen_dem_file", ///
	nogen keep(match master)
}
keep spid *died *status dd* adm *spstat*

//look at death month & year. don't have, so set date to last of month
gen dd_nhats=.
replace pd2yrdied=pd2yrdied+2010 if pd2yrdied>0
gen death_day=. 
forvalues i=2/6 {
	foreach j in mth yr {
		replace pd`i'`j'died=. if pd`i'`j'<0
}
	replace death_day=31 if inlist(pd`i'm,1,3,5,7,8,10,12)
	replace death_day=30 if inlist(pd`i'm,4,6,9,11)
	replace death_day=28 if inlist(pd`i'm,2)
	replace dd_nh=mdy(pd`i'mthdied,death_day,pd`i'yrdied) if pd`i'yrdied>0 & pd`i'yrdied!=.
}
gen dd_present_nhats=dd_nhats!=.
gen dd_present_claims=dd_claims!=.
gen mcdy=year(dd_claims)
gen nhdy=year(dd_nhats)
tab dd_pres*,m
tab nhdy mcdy,m
gen match=month(dd_nhats)==month(dd_claims) & year(dd_nhats)==year(dd_claims)
tab match
/*preserve
keep if dd_present_claims | nhdy<2013
*/
tab dd_pres*,m
tab nhdy mcdy,m
tab match
forvalues i=2/5 {
tab r`i'status
}

*keep if dd_present_n!=dd_present_c
tab match
gen last_ivw_date=.
forvalues i=2/6 {
tab r`i'status
replace last_ivw_date=mdy(r`i'spstatdtm,1,r`i'spstatdtyr) if inlist(r`i'status,60,61,63) 

}
format last_ivw dd_cl dd_nh %td
gen time=admit-dd_nhats
gen nhdm=month(dd_nhats)
gen mcdm=month(dd_claims)
gen conflict1=admit>dd_nhats & admit!=.
gen conflict1a=admit>dd_claims & admit!=.
gen nhonly=dd_present_nh & !dd_present_claims & nhdy<2013
gen conflict2=last_ivw>dd_claims & last_ivw!=.
gen claimsonly=dd_present_claims & nhdy>2012

preserve
keep if !missing(dd_claims) | nhdy<2013
//800 deaths from either claims or nhats, 621 (77.63%) match month and year
tab match
tab conflict1
//52 cases with a claim after death (nhats), 24 with a claim >31 days after death
tab conflict1a
//67 cases with a claim after death (claims), 25 with a claim>31 days after death

//following the method used for HRS, we will take claims death date.  if there's 
//a death date in nhats only (14 cases), that will be used.  if there's a claim 
//after the nhats death date, the last claim date is used.

restore
drop death_day
gen death_date=dd_claims
replace death_date=dd_nhats if dd_claims==.
replace death_date=last if last>dd_nhats & dd_claims==. & !missing(last)
label var death_date "SP death date, from tracker (day imputed)"
format death_date %td
gen death_month=month(death_date)
gen death_year=year(death_date)
foreach x in month year {
label var death_date "SP death `x', from claims or tracker"
}
gen death_source=1 if death_date==dd_claims 
replace death_source=2 if death_date==last & dd_claims==.
replace death_source=3 if death_date==dd_nhats & dd_claims==.
replace death_source=4 if death_date==dd_claims & dd_claims==dd_nhats 
replace death_source=. if missing(death_date)

label define death_source 1 "MBSF" 2 "last claim after NHATS DOD" 3 "NHATS" 4 "Both"
label values death_source death_source

keep spid death*
save "E:\nhats\data\CMS_DUA_28016\death_date.dta", replace



